{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "opts": {
        "template": "node_modules/tui-jsdoc-template"
    },
    "templates": {
        "logo": {
            "url": "../uni_logo.png",
            "width": "150px",
            "height": "58px",
            "link": "https://kb.universablockchain.com"
        },
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "name": "U8 JS Doc",
        "footerText": "Universa blockchain protocol",
        "default": {
            "staticFiles": {
                "include": ["static/"]
            }
        },
        "css": [
            "styles/tui-doc.css"
        ]
    }
}
